<ul class="menu-root">
  <% ['api', 'style-guide'].indexOf(type) === -1 && site.pages.find({type: type}).sort('order').each(function (p) { %>
    <% var fileName = p.path.replace(/^.+?\/([\w-]+)\.html/, '$1') %>
    <% if (type === 'tutorials') { %>
      <% if (fileName === 'instructions') { %>
        <li><h3>教程</h3></li>
      <% } %>
      <% if (fileName === 'abouttestnet') { %>
        <li><h3>节点编程</h3></li>
      <% } %>
      <% if (fileName === 'situation') { %>
        <li><h3>Fibos.js</h3></li>
      <% } %>
      <% if (fileName === 'whatisabi') { %>
        <li><h3>智能合约</h3></li>
      <% } %>
      <% if (fileName === 'eos2fibos') { %>
        <li><h3>Token 合约</h3></li>
      <% } %>
    <% } %>
    <% if (type === 'manual') { %>
      <% if (fileName === 'index') { %>
        <li><h3>JS contract</h3></li>
      <% } %>
      <% if (fileName === 'nodeIndex') { %>
        <li><h3>fibos node</h3></li>
      <% } %>
      <% if (fileName === 'eosioIndex') { %>
        <li><h3>eosio contract</h3></li>
      <% } %>
      <% if (fileName === 'tokenIndex') { %>
        <li><h3>eosio.token contract</h3></li>
      <% } %>
      <% if (fileName === 'fibosIndex') { %>
        <li><h3>fibos.js</h3></li>
      <% } %>
      <% if (fileName === 'rpcIndex') { %>
        <li><h3>RPC API</h3></li>
      <% } %>
    <% } %>
    <li>
      <a href="<%- url_for(p.path) %>" class="sidebar-link<%- page.title === p.title ? ' current' : '' %><%- p.is_new ? ' new' : '' %>"><%- p.title %></a>
    </li>
  <% }) %>
</ul>
